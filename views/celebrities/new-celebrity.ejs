<h2>Famous Person</h2>

<form action="/celebrities/create" method="POST">
  <label for="name"> Name:<input type="text" name="name" /> </label>
  <label for="occupation">
    <select name="occupation">
      Occupation:
      <option value="Actor">Actor</option>
      <option value="Singer">Singer</option>
      <option value="Unknown">Unknown</option>
    </select>
  </label>
  <label for="catchPhrase">
    Catch Phrase:<input type="text" name="catchPhrase" />
  </label>
  <button type="submit">Add famous</button>
</form>
